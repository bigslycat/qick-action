{
  "name": "quick-action",
  "version": "1.0.0",
  "description": "Flow support, no constants, really simple",
  "public": true,
  "main": "lib/bundle.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "test": "jest",
    "precommit": "yarn lint && yarn flow && yarn test",
    "build": "shx rm -f lib/bundle.js && rollup -c",
    "preversion": "git checkout master && yarn precommit",
    "postversion": "yarn run build && npm publish && git push origin master --follow-tags"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "shx": "^0.2.2"
  },
  "repository": "git@github.com:bigslycat/quick-action.git",
  "author": {
    "name": "Pavel Tereschenko",
    "email": "me@pavlik.pro",
    "url": "https://github.com/bigslycat"
  },
  "keywords": [
    "redux",
    "actions",
    "redux-act",
    "redux-actions",
    "flow",
    "flowtype",
    "reducer",
    "reducers"
  ],
  "license": "MIT"
}
